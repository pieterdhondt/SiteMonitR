<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <connectionStrings>
    <add name="AzureWebJobsDashboard"
         connectionString="DefaultEndpointsProtocol=https;AccountName=[YOUR_ACCOUNT_NAME];AccountKey=[YOUR_ACCOUNT_KEY]" />
    <add name="AzureWebJobsStorage"
         connectionString="DefaultEndpointsProtocol=https;AccountName=[YOUR_ACCOUNT_NAME];AccountKey=[YOUR_ACCOUNT_KEY]" />
  </connectionStrings>
  <appSettings>
    <add key="SiteMonitR.DashboardUrl" value="http://[YOUR_APP_URL]/"/>
  </appSettings>
</configuration>